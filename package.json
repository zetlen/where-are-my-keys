{
  "name": "where-are-my-keys",
  "version": "0.0.1",
  "description": "Utility for finding auth tokens and keys in your environment",
  "keywords": [
    "tokens",
    "keys",
    "auth",
    "security"
  ],
  "homepage": "https://github.com/zetlen/where-are-my-keys#readme",
  "bugs": {
    "url": "https://github.com/zetlen/where-are-my-keys/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zetlen/where-are-my-keys.git"
  },
  "license": "ISC",
  "author": "zetlen <zetlen@gmail.com>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "where-are-my-keys": "./dist/cli.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node scripts/test-runner.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "glob": "^11.0.1",
    "tsup": "^8.5.1",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}